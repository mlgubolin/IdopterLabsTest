<h1>Show Thread</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @thread.name %>
  </li>

  <li>
    <strong>Title:</strong>
    <%= @thread.title %>
  </li>

  <li>
    <strong>Body:</strong>
    <%= @thread.body %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.thread_path(@conn, :edit, @thread) %></span>
<span><%= link "Back", to: Routes.thread_path(@conn, :index) %></span>
